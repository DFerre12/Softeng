create table HappySong(
    song_id serial primary key,
    song_name varchar(100) not null,
	  artist_name varchar(50) not null
);

insert into HappySong(song_id, song_name, artist_name) values (default, 'Happy', 'Pharrell Williams');
insert into HappySong(song_id, song_name, artist_name) values (default, 'Juice', 'Lizzo');
insert into HappySong(song_id, song_name, artist_name) values (default, 'Uptown Funk', 'Mark Ronson, Bruno Mars');


create table SadSong(
	   song_id serial primary key,
	   song_name varchar(100) not null,
	   artist_name varchar(50) not null
);

insert into SadSong(song_id, song_name, artist_name) values (default, 'Sad Song', 'We The Kings, Elena Coats');
insert into SadSong(song_id, song_name, artist_name) values (default, 'Changes', 'XXXTENTACION');
insert into SadSong(song_id, song_name, artist_name) values (default, 'How to Save a Life', 'The Fray');


create table LoveSong(
    	song_id serial primary key,
	    song_name varchar(100) not null,	
      artist_name varchar(50) not null
);

insert into LoveSong(song_id, song_name, artist_name) values (default, 'All of Me', 'John Legend');
insert into LoveSong(song_id, song_name, artist_name) values (default, 'Almost(Sweet Music)', 'Hozier');
insert into LoveSong(song_id, song_name, artist_name) values (default, 'Dont Start Now', 'Dua Lipa');



